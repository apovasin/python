a=str(input())
a1=a.split()
a1.sort()
a2=' '.join(a1)
print(a2)
a1.reverse()
a2=' '.join(a1)
print(a2)


a=str(input())
a1=set(a)
a2=sorted(a1)
a3=tuple(a2)
a4 = [int(item) for item in a3]
result=tuple(a4)
print(result)
a2.reverse()
a3=tuple(a2)
a4 = [int(item) for item in a3]
result=tuple(a4)
print(result)